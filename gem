    MainGui.Enabled = false
    SidebarOpen = false
    TweenService:Create(Sidebar, TweenInfo.new(0.3), {
        Position = UDim2.new(0, -300, 0, 60)
    }):Play()
    SidebarOverlay.Visible = false
end)

