    TweenService:Create(CloseBtn, TweenInfo.new(0.2), {
        BackgroundColor3 = Color3.fromRGB(35, 35, 45),
        TextColor3 = Color3.fromRGB(200, 200, 210)
    }):Play()
    TweenService:Create(CloseStroke, TweenInfo.new(0.2), {
        Transparency = 0.5
    }):Play()
end)

