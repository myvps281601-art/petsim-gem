    Noclip = state
end)

CreateToggle("âœˆï¸ Fly", "UÃ§ma modu (WASD + Space/Shift)", false, function(state)
    Fly = state
end)

CreateToggle("ðŸ‘ï¸ ESP", "Oyun objelerini gÃ¶ster", false, function(state)
    ESP = state
end)

-- Fly System
local BodyVelocity = Instance.new("BodyVelocity")
BodyVelocity.MaxForce = Vector3.new(40000, 40000, 40000)
BodyVelocity.Velocity = Vector3.new(0, 0, 0)
if HumanoidRootPart then
    BodyVelocity.Parent = HumanoidRootPart
end

RunService.RenderStepped:Connect(function()
    if Character and HumanoidRootPart and Fly then
        if not BodyVelocity.Parent then
            BodyVelocity.Parent = HumanoidRootPart
        end
        BodyVelocity.MaxForce = Vector3.new(40000, 40000, 40000)
        BodyVelocity.Velocity = Camera.CFrame.LookVector * (UserInputService:IsKeyDown(Enum.KeyCode.W) and 50 or 0) +
                                 Camera.CFrame.RightVector * (UserInputService:IsKeyDown(Enum.KeyCode.D) and 50 or 0) +
                                 Camera.CFrame.RightVector * (UserInputService:IsKeyDown(Enum.KeyCode.A) and -50 or 0) +
                                 Camera.CFrame.LookVector * (UserInputService:IsKeyDown(Enum.KeyCode.S) and -50 or 0) +
                                 Vector3.new(0, (UserInputService:IsKeyDown(Enum.KeyCode.Space) and 50 or 0) + (UserInputService:IsKeyDown(Enum.KeyCode.LeftShift) and -50 or 0), 0)
    else
        if BodyVelocity then
            BodyVelocity.MaxForce = Vector3.new(0, 0, 0)
            BodyVelocity.Velocity = Vector3.new(0, 0, 0)
        end
    end
end)

-- Noclip System
RunService.Stepped:Connect(function()
    if Character then
        if Noclip then
            for _, v in pairs(Character:GetDescendants()) do
                if v:IsA("BasePart") and v.CanCollide then
                    v.CanCollide = false
                end
            end
        else
            for _, v in pairs(Character:GetDescendants()) do
                if v:IsA("BasePart") then
                    v.CanCollide = true
                end
            end
        end
    end
end)

-- Character Updates
LocalPlayer.CharacterAdded:Connect(function(char)
    Character = char
    HumanoidRootPart = Character:WaitForChild("HumanoidRootPart")
    BodyVelocity.Parent = HumanoidRootPart
end)

-- Update ScrollFrame
ContentLayout:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
    ContentFrame.CanvasSize = UDim2.new(0, 0, 0, ContentLayout.AbsoluteContentSize.Y + 20)
end)

-- Open Key (F5)
UserInputService.InputBegan:Connect(function(input, gameProcessed)
    if not gameProcessed then
        if input.KeyCode == Enum.KeyCode.F5 then
            MainGui.Enabled = not MainGui.Enabled
        end
    end
end)

