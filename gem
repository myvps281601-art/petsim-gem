    InfiniteGems = state
    SetInfiniteGems(state)
end)

CreateToggle("ðŸ¥š Otomatik Egg AÃ§Ä±cÄ±", "YumurtalarÄ± otomatik olarak aÃ§ar", false, function(state)
    AutoEggOpener = state
    SetAutoEggOpener(state)
end)

