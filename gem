-- Sidebar Menu Items
local function CreateSidebarItem(title, icon, callback)
    local Item = Instance.new("TextButton")
    Item.Size = UDim2.new(1, -20, 0, 50)
    Item.BackgroundColor3 = Color3.fromRGB(20, 20, 28)
    Item.BorderSizePixel = 0
    Item.Text = icon .. " " .. title
    Item.TextColor3 = Color3.fromRGB(220, 220, 230)
    Item.TextSize = 16
    Item.Font = Enum.Font.GothamBold
    Item.AutoButtonColor = false
    Item.ZIndex = 16
    Item.Parent = SidebarContent
    
    local ItemCorner = Instance.new("UICorner")
    ItemCorner.CornerRadius = UDim.new(0, 10)
    ItemCorner.Parent = Item
    
    local ItemStroke = Instance.new("UIStroke")
    ItemStroke.Thickness = 1
    ItemStroke.Color = Color3.fromRGB(70, 150, 255)
    ItemStroke.Transparency = 0.7
    ItemStroke.Parent = Item
    
    Item.MouseEnter:Connect(function()
        TweenService:Create(Item, TweenInfo.new(0.2), {
            BackgroundColor3 = Color3.fromRGB(30, 30, 40),
            TextColor3 = Color3.fromRGB(70, 150, 255)
        }):Play()
        TweenService:Create(ItemStroke, TweenInfo.new(0.2), {
            Transparency = 0.3
        }):Play()
    end)
    
    Item.MouseLeave:Connect(function()
        TweenService:Create(Item, TweenInfo.new(0.2), {
            BackgroundColor3 = Color3.fromRGB(20, 20, 28),
            TextColor3 = Color3.fromRGB(220, 220, 230)
        }):Play()
        TweenService:Create(ItemStroke, TweenInfo.new(0.2), {
            Transparency = 0.7
        }):Play()
    end)
    
    Item.MouseButton1Click:Connect(function()
        if callback then callback() end
    end)
    
    return Item
end

