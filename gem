-- Professional Toggle Function
local function CreateToggle(title, description, initialState, callback)
    -- Ana Container (TÄ±klanabilir TextButton)
    local ToggleContainer = Instance.new("TextButton")
    ToggleContainer.Size = UDim2.new(1, 0, 0, 75)
    ToggleContainer.BackgroundColor3 = Color3.fromRGB(25, 25, 32)
    ToggleContainer.BorderSizePixel = 0
    ToggleContainer.Text = ""
    ToggleContainer.AutoButtonColor = false
    ToggleContainer.ZIndex = 7
    ToggleContainer.Parent = ContentFrame
    
    -- Gradient Background
    local ToggleGradient = Instance.new("UIGradient")
    ToggleGradient.Color = ColorSequence.new({
        ColorSequenceKeypoint.new(0, Color3.fromRGB(25, 25, 32)),
        ColorSequenceKeypoint.new(1, Color3.fromRGB(30, 30, 38))
    })
    ToggleGradient.Rotation = 90
    ToggleGradient.Parent = ToggleContainer
    
    local ToggleCorner = Instance.new("UICorner")
    ToggleCorner.CornerRadius = UDim.new(0, 12)
    ToggleCorner.Parent = ToggleContainer
    
    -- Premium Border with Glow
    local ToggleStroke = Instance.new("UIStroke")
    ToggleStroke.Thickness = 1.5
    ToggleStroke.Color = Color3.fromRGB(70, 150, 255)
    ToggleStroke.Transparency = 0.6
    ToggleStroke.Parent = ToggleContainer
    
    -- Title
    local ToggleTitle = Instance.new("TextLabel")
    ToggleTitle.Size = UDim2.new(1, -90, 0, 24)
    ToggleTitle.Position = UDim2.new(0, 16, 0, 12)
    ToggleTitle.BackgroundTransparency = 1
    ToggleTitle.Text = title
    ToggleTitle.TextColor3 = Color3.fromRGB(245, 245, 250)
    ToggleTitle.TextSize = 16
    ToggleTitle.Font = Enum.Font.GothamBold
    ToggleTitle.TextXAlignment = Enum.TextXAlignment.Left
    ToggleTitle.Parent = ToggleContainer
    
    -- Description
    local ToggleDesc = Instance.new("TextLabel")
    ToggleDesc.Size = UDim2.new(1, -90, 0, 18)
    ToggleDesc.Position = UDim2.new(0, 16, 0, 36)
    ToggleDesc.BackgroundTransparency = 1
    ToggleDesc.Text = description
    ToggleDesc.TextColor3 = Color3.fromRGB(160, 160, 170)
    ToggleDesc.TextSize = 12
    ToggleDesc.Font = Enum.Font.Gotham
    ToggleDesc.TextXAlignment = Enum.TextXAlignment.Left
    ToggleDesc.Parent = ToggleContainer
    
    -- Toggle Switch Background
    local ToggleBg = Instance.new("Frame")
    ToggleBg.Size = UDim2.new(0, 56, 0, 30)
    ToggleBg.Position = UDim2.new(1, -72, 0, 22)
    ToggleBg.BackgroundColor3 = initialState and Color3.fromRGB(70, 150, 255) or Color3.fromRGB(45, 45, 55)
    ToggleBg.BorderSizePixel = 0
    ToggleBg.ZIndex = 8
    ToggleBg.Parent = ToggleContainer
    
    -- Toggle Switch Glow
    local ToggleBgGlow = Instance.new("ImageLabel")
    ToggleBgGlow.Size = UDim2.new(1, 10, 1, 10)
    ToggleBgGlow.Position = UDim2.new(0, -5, 0, -5)
    ToggleBgGlow.BackgroundTransparency = 1
    ToggleBgGlow.Image = "rbxassetid://1316045217"
    ToggleBgGlow.ImageColor3 = initialState and Color3.fromRGB(70, 150, 255) or Color3.fromRGB(45, 45, 55)
    ToggleBgGlow.ImageTransparency = 0.9
    ToggleBgGlow.ScaleType = Enum.ScaleType.Slice
    ToggleBgGlow.SliceCenter = Rect.new(10, 10, 118, 118)
    ToggleBgGlow.ZIndex = 7
    ToggleBgGlow.Parent = ToggleBg
    
    -- Toggle Switch'i tÄ±klanabilir yap
    local ToggleSwitchBtn = Instance.new("TextButton")
    ToggleSwitchBtn.Size = UDim2.new(0, 75, 0, 45)
    ToggleSwitchBtn.Position = UDim2.new(1, -80, 0, 15)
    ToggleSwitchBtn.BackgroundTransparency = 1
    ToggleSwitchBtn.BorderSizePixel = 0
    ToggleSwitchBtn.Text = ""
    ToggleSwitchBtn.ZIndex = 10
    ToggleSwitchBtn.Parent = ToggleContainer
    
    local ToggleBgCorner = Instance.new("UICorner")
    ToggleBgCorner.CornerRadius = UDim.new(0, 15)
    ToggleBgCorner.Parent = ToggleBg
    
    -- Toggle Switch Border
    local ToggleBgStroke = Instance.new("UIStroke")
    ToggleBgStroke.Thickness = 1.5
    ToggleBgStroke.Color = initialState and Color3.fromRGB(100, 180, 255) or Color3.fromRGB(60, 60, 70)
    ToggleBgStroke.Transparency = 0.3
    ToggleBgStroke.Parent = ToggleBg
    
    -- Toggle Switch Dot
    local ToggleDot = Instance.new("Frame")
    ToggleDot.Size = UDim2.new(0, 24, 0, 24)
    ToggleDot.Position = initialState and UDim2.new(1, -27, 0, 3) or UDim2.new(0, 3, 0, 3)
    ToggleDot.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    ToggleDot.BorderSizePixel = 0
    ToggleDot.ZIndex = 9
    ToggleDot.Parent = ToggleBg
    
    -- Dot Glow
    local ToggleDotGlow = Instance.new("ImageLabel")
    ToggleDotGlow.Size = UDim2.new(1, 8, 1, 8)
    ToggleDotGlow.Position = UDim2.new(0, -4, 0, -4)
    ToggleDotGlow.BackgroundTransparency = 1
    ToggleDotGlow.Image = "rbxassetid://1316045217"
    ToggleDotGlow.ImageColor3 = Color3.fromRGB(255, 255, 255)
    ToggleDotGlow.ImageTransparency = 0.85
    ToggleDotGlow.ScaleType = Enum.ScaleType.Slice
    ToggleDotGlow.SliceCenter = Rect.new(10, 10, 118, 118)
    ToggleDotGlow.ZIndex = 8
    ToggleDotGlow.Parent = ToggleDot
    
    local ToggleDotCorner = Instance.new("UICorner")
    ToggleDotCorner.CornerRadius = UDim.new(0, 12)
    ToggleDotCorner.Parent = ToggleDot
    
    -- Hover Effect
    local HoverFrame = Instance.new("Frame")
    HoverFrame.Size = UDim2.new(1, 0, 1, 0)
    HoverFrame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    HoverFrame.BackgroundTransparency = 1
    HoverFrame.BorderSizePixel = 0
    HoverFrame.ZIndex = 1
    HoverFrame.Parent = ToggleContainer
    
    local HoverCorner = Instance.new("UICorner")
    HoverCorner.CornerRadius = UDim.new(0, 10)
    HoverCorner.Parent = HoverFrame
    
    local state = initialState
    
    local function UpdateToggle()
        if state then
            TweenService:Create(ToggleBg, TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
                BackgroundColor3 = Color3.fromRGB(70, 150, 255)
            }):Play()
            TweenService:Create(ToggleDot, TweenInfo.new(0.3, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {
                Position = UDim2.new(1, -27, 0, 3)
            }):Play()
            TweenService:Create(ToggleBgGlow, TweenInfo.new(0.3), {
                ImageColor3 = Color3.fromRGB(70, 150, 255),
                ImageTransparency = 0.7
            }):Play()
            TweenService:Create(ToggleBgStroke, TweenInfo.new(0.3), {
                Color = Color3.fromRGB(100, 180, 255),
                Transparency = 0.1
            }):Play()
        else
            TweenService:Create(ToggleBg, TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
                BackgroundColor3 = Color3.fromRGB(45, 45, 55)
            }):Play()
            TweenService:Create(ToggleDot, TweenInfo.new(0.3, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {
                Position = UDim2.new(0, 3, 0, 3)
            }):Play()
            TweenService:Create(ToggleBgGlow, TweenInfo.new(0.3), {
                ImageColor3 = Color3.fromRGB(45, 45, 55),
                ImageTransparency = 0.9
            }):Play()
            TweenService:Create(ToggleBgStroke, TweenInfo.new(0.3), {
                Color = Color3.fromRGB(60, 60, 70),
                Transparency = 0.5
            }):Play()
        end
        callback(state)
    end
    
    -- Hover Animations
    ToggleContainer.MouseEnter:Connect(function()
        TweenService:Create(HoverFrame, TweenInfo.new(0.2), {
            BackgroundTransparency = 0.93
        }):Play()
        TweenService:Create(ToggleStroke, TweenInfo.new(0.2), {
            Color = Color3.fromRGB(100, 180, 255),
            Transparency = 0.3
        }):Play()
    end)
    
    ToggleContainer.MouseLeave:Connect(function()
        TweenService:Create(HoverFrame, TweenInfo.new(0.2), {
            BackgroundTransparency = 1
        }):Play()
        TweenService:Create(ToggleStroke, TweenInfo.new(0.2), {
            Color = Color3.fromRGB(70, 150, 255),
            Transparency = 0.6
        }):Play()
    end)
    
    -- Toggle Click - Container'a tÄ±klama
    ToggleContainer.MouseButton1Click:Connect(function()
        state = not state
        UpdateToggle()
    end)
    
    -- Toggle Click - Switch'e tÄ±klama
    ToggleSwitchBtn.MouseButton1Click:Connect(function()
        state = not state
        UpdateToggle()
    end)
    
    return ToggleContainer
end

-- Gem System
local function FindGemValue()
    local gems = {}
    local mainGem = nil
    
    local leaderstats = LocalPlayer:FindFirstChild("leaderstats")
    if leaderstats then
        for _, stat in pairs(leaderstats:GetChildren()) do
            if stat:IsA("IntValue") or stat:IsA("NumberValue") or stat:IsA("StringValue") then
                local nameLower = stat.Name:lower()
                if string.find(nameLower, "gem") or 
                   string.find(nameLower, "gems") or
                   string.find(nameLower, "diamond") or
                   string.find(nameLower, "diamonds") then
                    table.insert(gems, stat)
                    if not mainGem then
                        mainGem = stat
                    end
                end
            end
        end
    end
    
    local playerGui = LocalPlayer:FindFirstChild("PlayerGui")
    if playerGui then
        for _, gui in pairs(playerGui:GetDescendants()) do
            if (gui:IsA("IntValue") or gui:IsA("NumberValue") or gui:IsA("StringValue")) then
                local nameLower = gui.Name:lower()
                if string.find(nameLower, "gem") or 
                   string.find(nameLower, "gems") or
                   string.find(nameLower, "diamond") or
                   string.find(nameLower, "diamonds") then
                    table.insert(gems, gui)
                    if not mainGem then
                        mainGem = gui
                    end
                end
            end
        end
    end
    
    for _, obj in pairs(Workspace:GetDescendants()) do
        if (obj:IsA("IntValue") or obj:IsA("NumberValue") or obj:IsA("StringValue")) then
            local nameLower = obj.Name:lower()
            if string.find(nameLower, "gem") or 
               string.find(nameLower, "gems") or
               string.find(nameLower, "diamond") or
               string.find(nameLower, "diamonds") then
                table.insert(gems, obj)
                if not mainGem then
                    mainGem = obj
                end
            end
        end
    end
    
    for _, obj in pairs(ReplicatedStorage:GetDescendants()) do
        if (obj:IsA("IntValue") or obj:IsA("NumberValue") or obj:IsA("StringValue")) then
            local nameLower = obj.Name:lower()
            if string.find(nameLower, "gem") or 
               string.find(nameLower, "gems") or
               string.find(nameLower, "diamond") or
               string.find(nameLower, "diamonds") then
                table.insert(gems, obj)
                if not mainGem then
                    mainGem = obj
                end
            end
        end
    end
    
    return mainGem, gems
end

local GemTextLabels = {} -- TextLabel cache
local GemUpdateConnection = nil
local GemTextLabelConnections = {}
local MaxGemsValue = 99999000000000 -- 99999B iÃ§in (Global)

-- Format: 99999B (Global Function)
local function FormatGems(value)
    if not value then value = MaxGemsValue end
    local billions = value / 1000000000
    if billions >= 1 then
        -- 99999B formatÄ±
        return string.format("%.0f", billions) .. "B"
    else
        -- 99999M formatÄ± (milyon)
        local millions = value / 1000000
        if millions >= 1 then
            return string.format("%.0f", millions) .. "M"
        else
            return string.format("%.0f", value)
        end
    end
end

local function SetInfiniteGems(enabled)
    InfiniteGems = enabled
    
    if enabled then
        print("ðŸ’ŽðŸ’ŽðŸ’Ž GEM SÄ°STEMÄ° AKTÄ°F EDÄ°LDÄ° ðŸ’ŽðŸ’ŽðŸ’Ž")
        
        -- Gem deÄŸerlerini bul
        GemValue, AllGemValues = FindGemValue()
        
        print("ðŸ’Ž " .. #AllGemValues .. " gem deÄŸeri bulundu!")
        for i, val in ipairs(AllGemValues) do
            print("  - " .. i .. ". " .. val.Name .. " (" .. val.ClassName .. ")")
        end
        
        -- SUPER AGRESIF GEM GÃœNCELLEME SÄ°STEMÄ°
        local function UpdateAllGems()
            if not InfiniteGems then return end
            
            -- 1. TÃœM GEM DEÄžERLERÄ°NÄ° SET ET
            for _, val in ipairs(AllGemValues) do
                if val and val.Parent then
                    pcall(function()
                        if val:IsA("IntValue") or val:IsA("NumberValue") then
                            val.Value = MaxGemsValue
                        elseif val:IsA("StringValue") then
                            val.Value = tostring(MaxGemsValue)
                        end
                    end)
                end
            end
            
            -- 2. ANA GEM DEÄžERÄ°NÄ° SET ET
            if GemValue and GemValue.Parent then
                pcall(function()
                    if GemValue:IsA("IntValue") or GemValue:IsA("NumberValue") then
                        GemValue.Value = MaxGemsValue
                    elseif GemValue:IsA("StringValue") then
                        GemValue.Value = tostring(MaxGemsValue)
                    end
                end)
            end
            
            -- 3. LEADERSTATS GEM DEÄžERLERÄ°NÄ° KONTROL ET VE GÃœNCELLE
            local leaderstats = LocalPlayer:FindFirstChild("leaderstats")
            if leaderstats then
                for _, stat in pairs(leaderstats:GetChildren()) do
                    if stat:IsA("IntValue") or stat:IsA("NumberValue") then
                        local nameLower = stat.Name:lower()
                        if string.find(nameLower, "gem") or 
                           string.find(nameLower, "gems") or
                           string.find(nameLower, "diamond") or
                           string.find(nameLower, "diamonds") then
                            pcall(function()
                                stat.Value = MaxGemsValue
                            end)
                            
                            -- Yeni bulunan gem deÄŸerini listeye ekle
                            local alreadyAdded = false
                            for _, val in ipairs(AllGemValues) do
                                if val == stat then
                                    alreadyAdded = true
                                    break
                                end
                            end
                            if not alreadyAdded then
                                table.insert(AllGemValues, stat)
                                print("ðŸ’Ž Yeni gem deÄŸeri eklendi: " .. stat.Name)
                            end
                        end
                    end
                end
            end
            
            -- 4. PLAYERGUI GEM DEÄžERLERÄ°NÄ° KONTROL ET VE GÃœNCELLE
            local playerGui = LocalPlayer:FindFirstChild("PlayerGui")
            if playerGui then
                for _, gui in pairs(playerGui:GetDescendants()) do
                    pcall(function()
                        -- Value objelerini kontrol et
                        if (gui:IsA("IntValue") or gui:IsA("NumberValue") or gui:IsA("StringValue")) then
                            local nameLower = gui.Name:lower()
                            if string.find(nameLower, "gem") or 
                               string.find(nameLower, "gems") or
                               string.find(nameLower, "diamond") or
                               string.find(nameLower, "diamonds") then
                                if gui:IsA("IntValue") or gui:IsA("NumberValue") then
                                    gui.Value = MaxGemsValue
                                elseif gui:IsA("StringValue") then
                                    gui.Value = tostring(MaxGemsValue)
                                end
                                
                                -- Yeni bulunan gem deÄŸerini listeye ekle
                                local alreadyAdded = false
                                for _, val in ipairs(AllGemValues) do
                                    if val == gui then
                                        alreadyAdded = true
                                        break
                                    end
                                end
                                if not alreadyAdded then
                                    table.insert(AllGemValues, gui)
                                end
                            end
                        end
                        
                        -- TextLabel'larÄ± kontrol et - SUPER AGRESIF
                        if gui:IsA("TextLabel") or gui:IsA("TextButton") then
                            local text = tostring(gui.Text or "")
                            local nameLower = gui.Name:lower()
                            
                            -- SayÄ± iÃ§eren TextLabel'larÄ± bul (virgÃ¼llÃ¼ format dahil)
                            local cleanText = text:gsub(",", ""):gsub("%s+", "")
                            local numValue = tonumber(cleanText)
                            
                            -- EÄŸer sayÄ± ise ve gem gÃ¶sterimi olabilir
                            if numValue and numValue > 0 and numValue < MaxGemsValue then
                                local isGemDisplay = false
                                
                                -- 1. Ä°sim kontrolÃ¼
                                if string.find(nameLower, "gem") or 
                                   string.find(nameLower, "gems") or
                                   string.find(nameLower, "diamond") or
                                   string.find(nameLower, "diamonds") then
                                    isGemDisplay = true
                                end
                                
                                -- 2. Parent kontrolleri (10 seviye yukarÄ±ya kadar)
                                local current = gui.Parent
                                local depth = 0
                                while current and depth < 10 do
                                    local currentName = current.Name:lower()
                                    if string.find(currentName, "gem") or
                                       string.find(currentName, "diamond") or
                                       string.find(currentName, "currency") or
                                       string.find(currentName, "resource") or
                                       string.find(currentName, "stat") or
                                       string.find(currentName, "gui") or
                                       string.find(currentName, "menu") then
                                        isGemDisplay = true
                                        break
                                    end
                                    current = current.Parent
                                    depth = depth + 1
                                end
                                
                                -- 3. Sibling kontrolÃ¼ (ImageLabel/ImageButton varsa)
                                local parent = gui.Parent
                                if parent then
                                    for _, sibling in pairs(parent:GetChildren()) do
                                        if sibling ~= gui then
                                            if sibling:IsA("ImageLabel") or 
                                               sibling:IsA("ImageButton") then
                                                isGemDisplay = true
                                                break
                                            end
                                        end
                                    end
                                end
                                
                                -- 4. EÄŸer gem gÃ¶sterimi ise GÃœNCELLE
                                if isGemDisplay then
                                    -- Format: "99999B"
                                    local formatted = FormatGems(MaxGemsValue)
                                    
                                    -- TextLabel'Ä± gÃ¼ncelle
                                    gui.Text = formatted
                                    
                                    -- TextLabel'Ä± cache'e ekle
                                    if not GemTextLabels[gui] then
                                        GemTextLabels[gui] = true
                                    end
                                end
                            end
                        end
                    end)
                end
            end
        end
        
        -- SUPER AGRESIF TEXTLABEL HOOK SÄ°STEMÄ°
        local function HookAllTextLabels()
            if not InfiniteGems then return end
            
            local playerGui = LocalPlayer:FindFirstChild("PlayerGui")
            if not playerGui then return end
            
            for _, gui in pairs(playerGui:GetDescendants()) do
                pcall(function()
                    if (gui:IsA("TextLabel") or gui:IsA("TextButton")) and not GemTextLabelConnections[gui] then
                        local text = tostring(gui.Text or "")
                        local nameLower = gui.Name:lower()
                        local cleanText = text:gsub(",", ""):gsub("%s+", "")
                        local numValue = tonumber(cleanText)
                        
                        -- SayÄ± iÃ§eren TextLabel'larÄ± bul
                        if numValue and numValue > 0 and numValue < MaxGemsValue then
                            local isGemDisplay = false
                            
                            -- Gem kontrolÃ¼
                            if string.find(nameLower, "gem") or 
                               string.find(nameLower, "gems") or
                               string.find(nameLower, "diamond") or
                               string.find(nameLower, "diamonds") then
                                isGemDisplay = true
                            end
                            
                            -- Parent kontrolÃ¼
                            local current = gui.Parent
                            local depth = 0
                            while current and depth < 10 do
                                local currentName = current.Name:lower()
                                if string.find(currentName, "gem") or
                                   string.find(currentName, "diamond") or
                                   string.find(currentName, "currency") or
                                   string.find(currentName, "resource") or
                                   string.find(currentName, "stat") then
                                    isGemDisplay = true
                                    break
                                end
                                current = current.Parent
                                depth = depth + 1
                            end
                            
                            -- Sibling kontrolÃ¼ (ImageLabel varsa)
                            local parent = gui.Parent
                            if parent then
                                for _, sibling in pairs(parent:GetChildren()) do
                                    if sibling:IsA("ImageLabel") or sibling:IsA("ImageButton") then
                                        isGemDisplay = true
                                        break
                                    end
                                end
                            end
                            
                            -- EÄŸer gem gÃ¶sterimi ise hook et
                            if isGemDisplay then
                                GemTextLabelConnections[gui] = true
                                GemTextLabels[gui] = true
                                
                                -- Text deÄŸiÅŸikliklerini dinle
                                gui:GetPropertyChangedSignal("Text"):Connect(function()
                                    if InfiniteGems then
                                        wait()
                                        local currentText = tostring(gui.Text or "")
                                        local currentNum = tonumber(currentText:gsub(",", ""))
                                        if currentNum and currentNum < MaxGemsValue then
                                            local formatted = FormatGems(MaxGemsValue)
                                            pcall(function()
                                                gui.Text = formatted
                                            end)
                                        end
                                    end
                                end)
                            end
                        end
                    end
                end)
            end
        end
        
        -- HER FRAME GÃœNCELLE - SUPER AGRESIF
        GemUpdateConnection = RunService.RenderStepped:Connect(function()
            if InfiniteGems then
                UpdateAllGems()
                HookAllTextLabels()
                
                -- Cached TextLabel'larÄ± sÃ¼rekli gÃ¼ncelle
                for textLabel, _ in pairs(GemTextLabels) do
                    if textLabel and textLabel.Parent then
                        pcall(function()
                            local currentText = tostring(textLabel.Text or "")
                            local currentNum = tonumber(currentText:gsub(",", ""):gsub("B", ""):gsub("M", ""):gsub("T", ""))
                            if not currentNum or currentNum < MaxGemsValue then
                                textLabel.Text = FormatGems(MaxGemsValue)
                            end
                        end)
                    end
                end
            end
        end)
        
        -- AYRICA SPAWN Ä°LE DE GÃœNCELLE (Ã‡Ä°FT GÃœVENLÄ°K)
        spawn(function()
            while InfiniteGems do
                wait(0.01) -- Her 10ms'de bir
                UpdateAllGems()
                HookAllTextLabels()
            end
        end)
        
        -- HER SANÄ°YE YENÄ° TEXTLABEL'LARI ARA
        spawn(function()
            while InfiniteGems do
                wait(1)
                HookAllTextLabels()
            end
        end)
        
        -- Hook RemoteEvents (Agresif)
        local function HookRemoteEvents()
            for _, obj in pairs(ReplicatedStorage:GetDescendants()) do
                if obj:IsA("RemoteEvent") and not HookedRemotes[obj] then
                    HookedRemotes[obj] = true
                    
                    local oldFireServer = obj.FireServer
                    obj.FireServer = function(self, ...)
                        local args = {...}
                        
                        if InfiniteGems then
                            -- Ã–NCE gem deÄŸerlerini yÃ¼kselt (99999B)
                            local remoteMaxGems = MaxGemsValue
                            
                            -- TÃ¼m gem deÄŸerlerini yÃ¼kselt
                            for _, val in ipairs(AllGemValues) do
                                if val and val.Parent then
                                    pcall(function()
                                        if val:IsA("IntValue") or val:IsA("NumberValue") then
                                            val.Value = remoteMaxGems
                                        elseif val:IsA("StringValue") then
                                            val.Value = tostring(remoteMaxGems)
                                        end
                                    end)
                                end
                            end
                            
                            if GemValue and GemValue.Parent then
                                pcall(function()
                                    if GemValue:IsA("IntValue") or GemValue:IsA("NumberValue") then
                                        GemValue.Value = remoteMaxGems
                                    elseif GemValue:IsA("StringValue") then
                                        GemValue.Value = tostring(remoteMaxGems)
                                    end
                                end)
                            end
                            
                            -- Leaderstats'taki gem deÄŸerlerini de yÃ¼kselt
                            local leaderstats = LocalPlayer:FindFirstChild("leaderstats")
                            if leaderstats then
                                for _, stat in pairs(leaderstats:GetChildren()) do
                                    if stat:IsA("IntValue") or stat:IsA("NumberValue") then
                                        local nameLower = stat.Name:lower()
                                        if string.find(nameLower, "gem") or 
                                           string.find(nameLower, "gems") or
                                           string.find(nameLower, "diamond") or
                                           string.find(nameLower, "diamonds") then
                                            pcall(function()
                                                stat.Value = remoteMaxGems
                                            end)
                                        end
                                    end
                                end
                            end
                            
                            -- TextLabel'larÄ± da gÃ¼ncelle
                            for textLabel, _ in pairs(GemTextLabels) do
                                if textLabel and textLabel.Parent then
                                    pcall(function()
                                        textLabel.Text = FormatGems(remoteMaxGems)
                                    end)
                                end
                            end
                            
                            -- Trade ve Pet AlÄ±m Ä°ÅŸlemlerini Kontrol Et
                            local isTrade = false
                            local isPetPurchase = false
                            local isHugePet = false
                            
                            for i, arg in ipairs(args) do
                                local argStr = tostring(arg):lower()
                                
                                -- Trade kontrolÃ¼
                                if string.find(argStr, "trade") or 
                                   string.find(argStr, "exchange") or
                                   string.find(argStr, "offer") then
                                    isTrade = true
                                    print("ðŸ’Ž Trade iÅŸlemi tespit edildi!")
                                end
                                
                                -- Pet alÄ±m kontrolÃ¼
                                if string.find(argStr, "buy") or 
                                   string.find(argStr, "purchase") or
                                   string.find(argStr, "pet") or
                                   string.find(argStr, "hatch") then
                                    isPetPurchase = true
                                    print("ðŸ’Ž Pet alÄ±m iÅŸlemi tespit edildi!")
                                end
                                
                                -- Huge pet kontrolÃ¼
                                if string.find(argStr, "huge") or 
                                   string.find(argStr, "titanic") then
                                    isHugePet = true
                                    print("ðŸ’Ž Huge pet alÄ±m iÅŸlemi tespit edildi!")
                                end
                                
                                -- Negatif deÄŸer kontrolÃ¼ (harcama engelleme)
                                if type(arg) == "number" and arg < 0 then
                                    -- Trade veya pet alÄ±mÄ±nda gem harcamasÄ±nÄ± engelleme
                                    if isTrade or isPetPurchase or isHugePet then
                                        print("ðŸš« Gem harcama engellendi (Trade/Pet): " .. tostring(arg))
                                        -- Gem deÄŸerlerini tekrar yÃ¼kselt
                                        for _, val in ipairs(AllGemValues) do
                                            if val and val.Parent then
                                                pcall(function()
                                                    if val:IsA("IntValue") or val:IsA("NumberValue") then
                                                        val.Value = remoteMaxGems
                                                    end
                                                end)
                                            end
                                        end
                                        if GemValue and GemValue.Parent then
                                            pcall(function()
                                                if GemValue:IsA("IntValue") or GemValue:IsA("NumberValue") then
                                                    GemValue.Value = remoteMaxGems
                                                end
                                            end)
                                        end
                                        -- HarcamayÄ± engelleme - normal iÅŸleme devam et ama gem deÄŸerlerini yÃ¼ksek tut
                                    end
                                end
                                
                                -- String bazlÄ± harcama kontrolÃ¼
                                if type(arg) == "string" then
                                    local lower = string.lower(arg)
                                    if string.find(lower, "spend") or 
                                       string.find(lower, "remove") or
                                       string.find(lower, "deduct") then
                                        -- Trade veya pet alÄ±mÄ±nda deÄŸilse engelle
                                        if not (isTrade or isPetPurchase or isHugePet) then
                                            print("ðŸš« Gem harcama isteÄŸi engellendi: " .. tostring(arg))
                                            return
                                        end
                                    end
                                end
                            end
                            
                            -- Trade veya pet alÄ±mÄ±nda gem deÄŸerlerini ekstra yÃ¼kselt
                            if isTrade or isPetPurchase or isHugePet then
                                -- Gem deÄŸerlerini tekrar yÃ¼kselt (ekstra gÃ¼venlik)
                                for _, val in ipairs(AllGemValues) do
                                    if val and val.Parent then
                                        pcall(function()
                                            if val:IsA("IntValue") or val:IsA("NumberValue") then
                                                val.Value = remoteMaxGems
                                            elseif val:IsA("StringValue") then
                                                val.Value = tostring(remoteMaxGems)
                                            end
                                        end)
                                    end
                                end
                                if GemValue and GemValue.Parent then
                                    pcall(function()
                                        if GemValue:IsA("IntValue") or GemValue:IsA("NumberValue") then
                                            GemValue.Value = remoteMaxGems
                                        elseif GemValue:IsA("StringValue") then
                                            GemValue.Value = tostring(remoteMaxGems)
                                        end
                                    end)
                                end
                                
                                -- TextLabel'larÄ± da gÃ¼ncelle
                                for textLabel, _ in pairs(GemTextLabels) do
                                    if textLabel and textLabel.Parent then
                                        pcall(function()
                                            textLabel.Text = FormatGems(remoteMaxGems)
                                        end)
                                    end
                                end
                            end
                        end
                        
                        return oldFireServer(self, ...)
                    end
                elseif obj:IsA("RemoteFunction") and not HookedRemotes[obj] then
                    HookedRemotes[obj] = true
                    
                    local oldInvokeServer = obj.InvokeServer
                    obj.InvokeServer = function(self, ...)
                        local args = {...}
                        
                        if InfiniteGems then
                            local remoteMaxGems = MaxGemsValue -- 99999B
                            
                            -- Ã–NCE gem deÄŸerlerini yÃ¼kselt
                            for _, val in ipairs(AllGemValues) do
                                if val and val.Parent then
                                    pcall(function()
                                        if val:IsA("IntValue") or val:IsA("NumberValue") then
                                            val.Value = remoteMaxGems
                                        elseif val:IsA("StringValue") then
                                            val.Value = tostring(remoteMaxGems)
                                        end
                                    end)
                                end
                            end
                            if GemValue and GemValue.Parent then
                                pcall(function()
                                    if GemValue:IsA("IntValue") or GemValue:IsA("NumberValue") then
                                        GemValue.Value = remoteMaxGems
                                    elseif GemValue:IsA("StringValue") then
                                        GemValue.Value = tostring(remoteMaxGems)
                                    end
                                end)
                            end
                            
                            -- Leaderstats'taki gem deÄŸerlerini de yÃ¼kselt
                            local leaderstats = LocalPlayer:FindFirstChild("leaderstats")
                            if leaderstats then
                                for _, stat in pairs(leaderstats:GetChildren()) do
                                    if stat:IsA("IntValue") or stat:IsA("NumberValue") then
                                        local nameLower = stat.Name:lower()
                                        if string.find(nameLower, "gem") or 
                                           string.find(nameLower, "gems") or
                                           string.find(nameLower, "diamond") or
                                           string.find(nameLower, "diamonds") then
                                            pcall(function()
                                                stat.Value = remoteMaxGems
                                            end)
                                        end
                                    end
                                end
                            end
                            
                            -- TextLabel'larÄ± da gÃ¼ncelle
                            for textLabel, _ in pairs(GemTextLabels) do
                                if textLabel and textLabel.Parent then
                                    pcall(function()
                                        textLabel.Text = FormatGems(remoteMaxGems)
                                    end)
                                end
                            end
                            
                            -- Trade ve Pet AlÄ±m Ä°ÅŸlemlerini Kontrol Et
                            local isTrade = false
                            local isPetPurchase = false
                            local isHugePet = false
                            
                            for i, arg in ipairs(args) do
                                local argStr = tostring(arg):lower()
                                
                                -- Trade kontrolÃ¼
                                if string.find(argStr, "trade") or 
                                   string.find(argStr, "exchange") or
                                   string.find(argStr, "offer") then
                                    isTrade = true
                                    print("ðŸ’Ž Trade iÅŸlemi tespit edildi (Invoke)!")
                                end
                                
                                -- Pet alÄ±m kontrolÃ¼
                                if string.find(argStr, "buy") or 
                                   string.find(argStr, "purchase") or
                                   string.find(argStr, "pet") or
                                   string.find(argStr, "hatch") then
                                    isPetPurchase = true
                                    print("ðŸ’Ž Pet alÄ±m iÅŸlemi tespit edildi (Invoke)!")
                                end
                                
                                -- Huge pet kontrolÃ¼
                                if string.find(argStr, "huge") or 
                                   string.find(argStr, "titanic") then
                                    isHugePet = true
                                    print("ðŸ’Ž Huge pet alÄ±m iÅŸlemi tespit edildi (Invoke)!")
                                end
                            end
                            
                            -- Trade veya pet alÄ±mÄ±nda gem deÄŸerlerini ekstra yÃ¼kselt
                            if isTrade or isPetPurchase or isHugePet then
                                -- Gem deÄŸerlerini tekrar yÃ¼kselt (ekstra gÃ¼venlik)
                                for _, val in ipairs(AllGemValues) do
                                    if val and val.Parent then
                                        pcall(function()
                                            if val:IsA("IntValue") or val:IsA("NumberValue") then
                                                val.Value = remoteMaxGems
                                            elseif val:IsA("StringValue") then
                                                val.Value = tostring(remoteMaxGems)
                                            end
                                        end)
                                    end
                                end
                                if GemValue and GemValue.Parent then
                                    pcall(function()
                                        if GemValue:IsA("IntValue") or GemValue:IsA("NumberValue") then
                                            GemValue.Value = remoteMaxGems
                                        elseif GemValue:IsA("StringValue") then
                                            GemValue.Value = tostring(remoteMaxGems)
                                        end
                                    end)
                                end
                                
                                -- TextLabel'larÄ± da gÃ¼ncelle
                                for textLabel, _ in pairs(GemTextLabels) do
                                    if textLabel and textLabel.Parent then
                                        pcall(function()
                                            textLabel.Text = FormatGems(remoteMaxGems)
                                        end)
                                    end
                                end
                            end
                            
                            -- Gem harcama isteklerini engelle (trade/pet alÄ±mÄ±nda deÄŸilse)
                            if not (isTrade or isPetPurchase or isHugePet) then
                                for i, arg in ipairs(args) do
                                    if type(arg) == "number" and arg < 0 then
                                        print("ðŸš« Gem harcama engellendi (Invoke): " .. tostring(arg))
                                        return 0, "CANCELLED"
                                    end
                                end
                            end
                        end
                        
                        return oldInvokeServer(self, ...)
                    end
                end
            end
        end
        
        HookRemoteEvents()
        
        ReplicatedStorage.DescendantAdded:Connect(function(obj)
            if (obj:IsA("RemoteEvent") or obj:IsA("RemoteFunction")) and InfiniteGems then
                HookRemoteEvents()
            end
        end)
        
        -- Workspace'teki yeni gem deÄŸerlerini dinle
        Workspace.DescendantAdded:Connect(function(obj)
            if (obj:IsA("IntValue") or obj:IsA("NumberValue") or obj:IsA("StringValue")) and InfiniteGems then
                local nameLower = obj.Name:lower()
                if string.find(nameLower, "gem") or 
                   string.find(nameLower, "gems") or
                   string.find(nameLower, "diamond") or
                   string.find(nameLower, "diamonds") then
                    table.insert(AllGemValues, obj)
                    print("ðŸ’Ž Yeni gem deÄŸeri bulundu: " .. obj.Name)
                end
            end
        end)
        
        -- Leaderstats eklendiÄŸinde tekrar ara
        LocalPlayer.ChildAdded:Connect(function(child)
            if child.Name == "leaderstats" and InfiniteGems then
                wait(1)
                GemValue, AllGemValues = FindGemValue()
                if #AllGemValues > 0 then
                    print("ðŸ’Ž Yeni gem deÄŸerleri bulundu! (" .. #AllGemValues .. " adet)")
                end
            end
        end)
    else
        print("âŒ SÄ±nÄ±rsÄ±z gem kapatÄ±ldÄ±")
    end
end

-- Auto Egg Opener
local function SetAutoEggOpener(enabled)
    AutoEggOpener = enabled
    
    if enabled then
        spawn(function()
            while AutoEggOpener do
                wait(1)
                
                local playerGui = LocalPlayer:FindFirstChild("PlayerGui")
                if playerGui then
                    for _, gui in pairs(playerGui:GetDescendants()) do
                        if gui:IsA("TextButton") or gui:IsA("ImageButton") then
                            local text = tostring(gui.Text or "")
                            local nameLower = gui.Name:lower()
                            
                            if string.find(text:lower(), "open") or
                               string.find(text:lower(), "hatch") or
                               string.find(nameLower, "egg") or
                               string.find(nameLower, "hatch") then
                                if not string.find(text:lower(), "max") then
                                    pcall(function()
                                        if gui:IsA("GuiButton") then
                                            gui:Activate()
                                        end
                                    end)
                                end
                            end
                        end
                    end
                end
            end
        end)
    end
end

